<h1>Contact Messages</h1>

<table class="table table-bordered table-striped">
	<thead>
		<tr>
			<th>E-mail</th>
			<th>Messages</th>
		</tr>
	</thead>
	<tbody>
		{{#each model as |contact|}}
			<tr>
				<td>{{contact.email}}</td>
				<td>{{contact.message}}</td>
			</tr>
		{{/each}}
	</tbody>
</table>
